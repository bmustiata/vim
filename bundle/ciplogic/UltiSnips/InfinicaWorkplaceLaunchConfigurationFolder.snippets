snippet metadata "Create a folder metadata node."
<metadata>
	<description>${1:Folder description}</description>
	<created>${2:`date +%d.%m.%Y`}</created>
	<created-by>${3:Bogdan Mustiata}</created-by>
	<last-modified>$3</last-modified>
	${0:}
</metadata>
endsnippet

snippet param "Create an HT Launch parameter."
<!--
	$1 - ${3:description}
-->
<parameter>
	<name>${1:name}</name>
	<value>${2:%}</value>
	<type>text/plain</type>
</parameter>
${0:}
endsnippet

